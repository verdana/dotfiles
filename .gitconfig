
[user]
    name  = Verdana Mu
    email = verdana.cn@gmail.com
[core]
    editor = vim
[alias]
    br   = branch
    ci   = commit
    cia  = commit --amend
    cii  = commit --interactive
    cim  = commit -a -m
    co   = checkout
    cob  = checkout -b
    desc = describe --always --contains --all
    last = log -l HEAD
    me   = merge
    one  = log --oneline --graph --decorate
    pa   = push --all
    rs   = remote show
    rso  = remote show origin
    sc   = stash clear
    sd   = stash drop
    sl   = stash list
    sp   = stash pop
    ss   = stash save
    st   = status
    un   = reset HEAD --
[color]
    ui = true
[man]
    viewer = eman
[diff]
    submodule = log
    tool      = vimdiff

